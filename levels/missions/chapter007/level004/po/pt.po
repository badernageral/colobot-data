msgstr "Passo a passo"

#. type: Bullet: '1)'
#: ../help/soluce.E.txt:2
#, no-wrap
msgid "Search the area for <a object|titan>titanium</a>."
msgstr "Pesquise na área por <a object|titan>titânio</a>."

#. type: Bullet: '2)'
#: ../help/soluce.E.txt:3
#, no-wrap
msgid "Build a <a object|convert>converter</a>."
msgstr "Construa um <a object|convert>conversor</a>."

#. type: Bullet: '3)'
#: ../help/soluce.E.txt:4
#, no-wrap
msgid "Build a <a object|research>research center</a>."
msgstr "Construa um <a object|research>centro de pesquisa</a>."

#. type: Bullet: '4)'
#: ../help/soluce.E.txt:5
#, no-wrap
msgid "Run the <thumper research program> <button 66/>."
msgstr "Execute o <thumper research program> <button 66/>."

#. type: Bullet: '5)'
#: ../help/soluce.E.txt:6
#, no-wrap
msgid "Manufacture a <a object|bottump>thumper</a>."
msgstr "Fabrique um <a object|bottump>batedor</a>."

#. type: Bullet: '6)'
#: ../help/soluce.E.txt:7
#, no-wrap
