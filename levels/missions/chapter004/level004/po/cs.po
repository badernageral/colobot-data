"\\tab;Temperature:      1.2C\n"
"\\tab;Atmosphere:       nitrogen\n"
"\\tab;Wind:             0.0 mps\n"
"\\tab;Titanium ore:     none\n"
"\\tab;Uranium ore:      none"
msgstr ""
"\\tab;Teplota:           1,2 °C\n"
"\\tab;Atmosféra:         dusík\n"
"\\tab;Vítr:              0,0 m/s\n"
"\\tab;Titanová ruda:     žádná\n"
"\\tab;Uranová ruda:      žádná"

#. type: Source code
